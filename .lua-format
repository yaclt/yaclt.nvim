indent_width: 2
continuation_indent_width: 4
no_use_tab: true
no_keep_simple_control_block_one_line: true
no_keep_simple_function_one_line: true
no_align_args: true
no_break_after_functioncall_lp: true
no_break_after_functioncall_rp: true
no_align_parameter: true
no_break_after_functiondef_lp: true
no_break_after_functiondef_rp: true
no_align_table_field: true
extra_sep_at_table_end: true
no_break_after_operator: true
double_quote_to_single_quote: true
no_spaces_inside_functiondef_parens: true
no_spaces_inside_functioncall_parens: true
spaces_inside_table_braces: true
spaces_around_equals_in_field: true
